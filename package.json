{
	"name": "electronics",
	"version": "1.0.0",
	"description": "Electronics calculators and cheat sheets.",
	"main": "index.js",
	"scripts": {
		"start": "npm-run-all --parallel build ",
		"build": "webpack -d && copy src\\index.html dist\\index.html && webpack-dev-server --mode development --content-base src\\ --hot --inline --port 8080",
		"build:prod": "webpack -p && copy src\\index.html dist\\index.html",
		"scss": "node-sass --watch src\\scss -o src\\css",
		"bootstrap": "copy node_modules\\bootstrap\\dist\\css\\bootstrap.css src\\css\\"
	},
	"author": "Halojen Productions",
	"license": "ISC",
	"dependencies": {
		"babel-core": "^6.26.3",
		"bootstrap": "^4.1.1",
		"classnames": "^2.2.6",
		"jquery": "^3.3.1",
		"npm-run-all": "^4.1.3",
		"popper": "^1.0.1",
		"popper.js": "^1.14.3",
		"postcss-loader": "^2.1.5",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-octicons": "^0.2.0",
		"url-parse": "^1.4.3"
	},
	"devDependencies": {
		"autoprefixer": "^8.6.4",
		"babel-loader": "^7.1.4",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"css-loader": "^0.28.11",
		"node-sass": "^4.9.0",
		"precss": "^3.1.2",
		"react-html-id": "^0.1.4",
		"react-svg-loader": "^2.1.0",
		"sass-loader": "^7.0.3",
		"style-loader": "^0.21.0",
		"webpack": "^4.14.0",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	}
}
